


<div class="news-section">
<div class="container">
    <h1>company list</h1>
      <div class="col-md-3 news-column">
      <ul>
        <% job.forEach((oneJob)=> { %>
      <li>
        <h2><%= oneJob.company %></h2>
          <a href="<%= oneJob.url %>"><p><%= oneJob.url %></p></a>
          <a href="/jobs/<%= oneJob._id %>"><h5>view details</h5></a>
        </li>
        <% }) %>
      </ul>
    </div>
</div>
</div>
<br>
<a href="/jobs/new">
 Add a company
</a>
